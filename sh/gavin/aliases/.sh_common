### if [[ ! $PATH =~ .*/bin/_npm/node_modules/.bin* ]]; then export PATH="$HOME/bin/_npm/node_modules/.bin:$PATH";fi; 
if [[ ! $PATH =~ .*/bin/node/lib/node_modules/.bin* ]]; then export PATH="$HOME/bin/node/lib/node_modules/.bin:$PATH";fi; 
if [[ ! $PATH =~ .*/.composer/vendor/bin* ]]; then export PATH="$HOME/.composer/vendor/bin:$PATH";fi;

alias vim="vim --startuptime ~/.vim.log "
alias vi=vim
alias h='history'
alias vin="vim -u NONE -N "
alias composer="php /home/gavin/bin/composer.phar"
alias gits="git status"
alias nodebin="cd ~/bin/node/lib/; ll;"
#alias node="$HOME/bin/node/bin/node";
alias npm="$HOME/bin/node/bin/npm";


########################################
source ~/.sh_custom;

if [ -n "$ZSH_VERSION" ]; then
   source ~/.zsh_common;
   source ~/.zsh_custom;
elif [ -n "$BASH_VERSION" ]; then
   source ~/.bash_common;
   source ~/.bash_custom;
else
   echo 'well, shoot.';
fi




