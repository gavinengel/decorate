[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  copy = <Primary>c
  paste = <Primary>v
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#630000"
    background_image = None
    copy_on_selection = True
    font = Monospace 8
    foreground_color = "#ffffff"
    scrollback_infinite = True
    use_system_font = False
